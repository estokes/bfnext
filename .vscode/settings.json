{
  "rust-analyzer.diagnostics.enable": true,

  "rust-analyzer.checkOnSave.command": "check",     // or "clippy"
  "rust-analyzer.check.extraArgs": ["--package=bflib", "--release"],
  "rust-analyzer.check.workspace": false,

  // Make sure no stray cfgs hide code paths
  "rust-analyzer.cargo.cfgs": [],

  // Re-enable these if you had disabled them to work around failures
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.procMacro.enable": true
}